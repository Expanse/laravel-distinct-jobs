{
    "name": "expanse/laravel-distinct-jobs",
    "description": "Allows queue jobs to be distinct",
    "authors": [
        {
            "name": "Glen Solsberry",
            "email": "gms8994@dp.cx"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": ">5.8.28"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench-core": "3.8.*",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Expanse\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Expanse\\Providers\\DistinctJobsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
